<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="s17.portlets.whitepages">
<body>
<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main">
    <div class="people-results">
        <tal:block tal:repeat="people view/people_list">
            <div>
                <div>
                    <h3><a tal:attributes="href people/getURL" tal:content="people/fullname"></a></h3>
                    <tal:email tal:condition="python:hasattr(people, 'email')">
                        <a tal:attributes="href python:'mailto:' + people.email" tal:content="people/email"></a>
                    </tal:email>
                    <!--<span>Extension: </span><span tal:content="people/extension"></span></br>-->
                </div>
            </div>
        </tal:block>
    </div>
    </tal:main-macro>
</metal:main>

</body>
</html>
